    "suppress-chunks-webpack-plugin": "^1.0.5",
    "vue-template-compiler": "~2.6.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "prettier",
      "prettier/vue"
    ],
    "rules": {
      "vue/name-property-casing": [
        "warn",
        "kebab-case"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "eslintIgnore": [
    "/dist/",
    "/*.js",
    "/tests/unit/coverage/",
    "src/vendor/libs/**/*.js",
    "/node_modules/**"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    ">= 1%",
    "last 2 versions",
    "not dead",
    "Chrome >= 45",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
